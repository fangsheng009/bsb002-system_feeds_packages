+owl_audio_player_get_protocol_info (OwlAudioPlayer *audio_player)
+{
+	g_return_val_if_fail (OWL_IS_AUDIO_PLAYER (audio_player), NULL);
+
+	return audio_player->priv->protocol_info;
+}
