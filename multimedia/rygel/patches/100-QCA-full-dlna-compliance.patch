+private class Time {
+    public static int from_string (string str) {
+        int hours, minutes, seconds;
+
+        str.scanf ("%d:%2d:%2d%*s", out hours, out minutes, out seconds);
+
+        return hours * 3600 + minutes * 60 + seconds;
+    }
+
