+	OwlAudioPlayer *audio_player = app_data;
+	g_return_if_fail (OWL_IS_AUDIO_PLAYER (audio_player));
+        /**
+         * Emit EOS signal.
+         **/
+        g_signal_emit (audio_player,
+                       signals[EOS],
+                       0);
+}
